<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>uco.doo.rugrats.uconnect</groupId>
  <artifactId>UconnectBusisness</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>UconnectBusisness</name>
  <description>UconnectBusiness es un componente del workspace que contiene  la lógica de negocio de la aplicación. Aquí se implementan los casos de uso y las reglas de negocio para interactuar con los datos y proporcionar servicios al usuario.</description>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <release>19</release>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
	  <dependency>
		<groupId>uco.doo.rugrats.uconnect</groupId>
  		<artifactId>UconnectCrossCutting</artifactId>
        <version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
		<groupId>uco.doo.rugrats.uconnect</groupId>
  		<artifactId>UconnectEntities</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
		  <groupId>uco.doo.rugrats.uconnect</groupId>
  		  <artifactId>UconnectDTO</artifactId>
  		  <version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
	  	 <groupId>uco.doo.rugrats.uconnect</groupId>
  		 <artifactId>UconnectData</artifactId>
  		 <version>0.0.1-SNAPSHOT</version>
	  </dependency>
  </dependencies>
</project>